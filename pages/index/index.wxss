.formBox {
  margin-top: 41rpx;
  padding: 40rpx;
  width: 100%;
  background: #f0f4f8;
  border-radius: 50rpx;
}

.whiteBox {
  width: 100%;
  transition: all 2s;
}

.sheet {
  display: flex;
  justify-content: space-between;
  height: 89rpx;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
}

.sheet .caption {
  color: #333333;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  position: relative;
  margin-left: 30rpx;
}

.sheet .caption:after {
  content: "";
  width: 12rpx;
  height: 12rpx;
  background: #4696eb;
  border-radius: 6rpx;
  position: absolute;
  left: -20rpx;
  top: 50%;
  transform: translateY(-50%);
}

.info .caption:after {
  background: #ff0000;
}

.type .caption:after {
  background: #0f2d5f;
}

.site .caption:after {
  width: 0;
}

.sheet .gut {
  color: #333333;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  position: relative;
}

.sheet .gut::before {
  content: ">";
  position: absolute;
  right: 0;
  color: #ddd;
  font-size: 30rpx;
}

.address .gut::before {
  content: "";
}

.site .gut::before {
  content: "";
}

.price {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 40rpx;
}

.price .current {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
