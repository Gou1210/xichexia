}

.price .discount {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}

.price .drop {
  width: 189rpx;
  height: 60rpx;
  background: #f0d208;
  border-radius: 25rpx;
  line-height: 60rpx;
  text-align: center;
}

.youhui {
  margin-top: 31rpx;
  width: 630rpx;
  height: 137rpx;
}

.washBtnBox {
