@keyframes pop {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
.pop {
  width: 100%;
  height: auto;
  /* position: fixed; */
  box-sizing: border-box;
  bottom: 0;
  left: 0;
  z-index: 999;
  border-radius: 35rpx 35rpx 0 0;
  display: flex;
  flex-flow: column nowrap;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
  animation: pop 0.2s;
}
.pop-title {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  font-weight: bolder;
  color: #333;
  text-align: left;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #ffe15c;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}
.pop-title text {
  height: 50rpx;
  width: auto;
  line-height: 50rpx;
}
.pop-title icon {
  color: #999;
}
.pop-list {
  width: 700rpx;
  height: auto;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
  flex-shrink: 0;
  margin: 20rpx 0;
}

.pop-list:nth-of-type(1) {
  margin-top: 45rpx;
}
.pop-list-box {
  width: 100%;
  height: auto;
  overflow-y: auto;
  display: flex;
  background: #fff;
  flex-flow: column nowrap;
  justify-content: flex-start;
  align-items: center;
  border-radius: 20rpx;
  transition: 0.2s all;
}
.pop-list-km {
  color: #999;
  font-size: 28rpx;
  flex: 1;
}
.pop-list-title {
  color: #333;
  flex-shrink: 0;
  font-size: 32rpx;
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.pop-list-btn {
  width: 140rpx;
  height: 50rpx;
  border-radius: 10rpx;
  flex-shrink: 0;
  text-align: center;
  line-height: 50rpx;
  font-size: 27rpx;
  background: #ffe15c;
}

.show-pop {
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  width: auto;
  height: 75rpx;
  line-height: 75rpx;
  padding: 0rpx 40rpx;
  color: #333;
  border-radius: 55rpx;
  background: #ffe15c;
}
.no-data {
  height: 180rpx;
  line-height: 180rpx;
  text-align: center;
  width: 100%;
  color: #999;
  font-size: 35rpx;
}
